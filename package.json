{
  "name": "expo-tamagui",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "expo run android",
    "check": "expo install --fix",
    "clean": "yarn cache clean --all && git clean -xdf",
    "doctor": "yarn dlx @yarnpkg/doctor && npx expo-doctor@latest",
    "ios": "expo run ios",
    "lint": "yarn dlx @biomejs/biome check --apply .",
    "start": "expo start -c",
    "update": "yarn upgrade-interactive",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "vscode": "yarn dlx @yarnpkg/sdks vscode",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.16",
    "@tamagui/config": "latest",
    "@tamagui/constants": "latest",
    "@tamagui/font-inter": "latest",
    "@tamagui/lucide-icons": "latest",
    "@tamagui/metro-plugin": "latest",
    "expo": "^50.0.13",
    "expo-asset": "^9.0.2",
    "expo-font": "^11.10.3",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.8",
    "expo-status-bar": "^1.11.1",
    "jotai": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.5",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "14.1.0",
    "react-native-web": "^0.19.10",
    "tamagui": "latest",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@biomejs/biome": "^1.6.1",
    "@expo/metro-runtime": "^3.1.3",
    "@types/react": "^18.2.66",
    "babel-preset-expo": "^10.0.1",
    "metro": "^0.80.6",
    "vercel": "latest"
  },
  "packageManager": "yarn@4.1.1"
}
